
3x5_db.inc
    3x5 card DB connection class
    handles most 3x5 DB interactions with explicitly formatted queries
	include:	rko_db.inc
	action:		(none)

cards.inc
    common values, functions, and classes
	include:	(none)
	action:		(none)

create_update_user.inc
    create_update_user class to handle user info processing
	include:	cards.inc
	action:		indexM.php

many_batch.inc
    many batch selector class
	include:	user.inc
			cards.inc
	action:		(none)

one_batch.inc
    one batch selector class
	include:	user.inc
			cards.inc
	action:		(none)

rko_db.inc
    RKO_DB simple DB connection class
    direct connection to DB with generalized operations
	include:	(none)
	action:		(none)

user.inc
    user class for managing uid and batch mappings
	include:	(none)
	action:		(none)

view.inc
    View class handles the batch view properties
	include:	user.inc
			cards.inc
			one_batch.inc
	action:		(none)

batches.php
    select batch operations
	include:	cards.inc
			user.inc
			3x5_db.inc
			one_batch.inc
	action:		(none)

create_user.php
    Create a new user and insert into DB
	include:	cards.inc
			3x5_db.inc
			create_update_user.inc
	action:		Location: login_user.php

csv_cards.php
    output the selected batches as a CSV file
	include:	user.inc
			view.inc
			cards.inc
			3x5_db.inc
	action:		sel_batches.php

del_batch.php
    Delete a whole batch from the DB
	include:	cards.inc
			user.inc
			3x5_db.inc
	action:		(none)

indexB.php
    options frame, shows access level and selection options
	include:	user.inc
			cards.inc
			3x5_db.inc
	action:		update_user.php
			batches.php
			del_batch.php
			list_batches.php
			sel_batches.php
			view_cards.php
			csv_cards.php

indexF.php
    sets up frames title/options/main
	include:	cards.inc
	action:		indexM.php
			login_user.php
			indexT.php
			indexB.php

indexM.php
    starting ``results'' page
	include:	user.inc
			cards.inc
	action:		login_user.php
			create_user.php

indexT.php
    title frame, shows username
	include:	user.inc
			cards.inc
	action:		create_user.php
			logout_user.php

list_batches.php
    List batch properties as a table
	include:	cards.inc
			user.inc
			3x5_db.inc
	action:		indexM.php

login_user.php
    prompts for username and password
	include:	user.inc
			cards.inc
			3x5_db.inc
	action:		create_user.php
			indexF.php

logout_user.php
    logs out the user and resets most values
	include:	user.inc
			cards.inc
	action:		Location: indexM.php
			Location: indexM.php

new_batch.php
    create a batch or update batch properties
	include:	cards.inc
			user.inc
			3x5_db.inc
			one_batch.inc
	action:		batches.php

sel_batches.php
    select which batch or set of batches for card handling
	include:	user.inc
			view.inc
			cards.inc
			many_batch.inc
			3x5_db.inc
	action:		Location: view_cards.php

update_user.php
	include:	user.inc
			cards.inc
			3x5_db.inc
			create_update_user.inc
	action:		(none)

view_cards.php
    view the individual cards as selected by the batch
	include:	user.inc
			view.inc
			cards.inc
			3x5_db.inc
	action:		(none)
